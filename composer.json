{
	"config": {
		"vendor-dir": "lib/external/packages",
		"allow-plugins": {
			"composer/installers": true,
			"oomphinc/composer-installers-extender": true,
			"fxp/composer-asset-plugin": false
		}
	},
	"extra": {
	   "installer-types": ["bower-asset", "npm-asset"],
	   "installer-paths": {
		   "lib/external/assets/composer/{$name}/": ["type:bower-asset", "type:npm-asset"]
	   }
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"minimum-stability": "RC",
	"name": "tigron/skeleton",
	"description": "Tigron Skeleton",
	"type": "project",
	"require": {
		"ext-soap": "*",

		"tigron/click-post": "*",
		"tigron/codabox-gateway": "*",
		"tigron/sepa-payments": "*",
		"tigron/skeleton-application-web": "0.*",
		"tigron/skeleton-console": "2.*",
		"tigron/skeleton-core": "4.*",
		"tigron/skeleton-database": "0.*",
		"tigron/skeleton-email": "0.*",
		"tigron/skeleton-error": "0.*",
		"tigron/skeleton-file-pdf": "0.*",
		"tigron/skeleton-file-picture": "0.*",
		"tigron/skeleton-file": "2.*",
		"tigron/skeleton-i18n": "4.*",
		"tigron/skeleton-migrate": "0.*",
		"tigron/skeleton-object": "1.*",
		"tigron/skeleton-package-api": "2.*",
		"tigron/skeleton-package-crud": "3.*",
		"tigron/skeleton-package-openapi": "dev-master",
		"tigron/skeleton-pager": "3.*",
		"tigron/skeleton-template-twig": "2.*",
		"tigron/skeleton-template": "0.*",
