{
	"config": {
		"vendor-dir": "lib/external/packages",
		"allow-plugins": {
			"composer/installers": true,
			"oomphinc/composer-installers-extender": true,
			"fxp/composer-asset-plugin": false
		}
	},
	"extra": {
	   "installer-types": ["bower-asset", "npm-asset"],
	   "installer-paths": {
		   "lib/external/assets/composer/{$name}/": ["type:bower-asset", "type:npm-asset"]
	   }
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],
	"minimum-stability": "RC",
	"name": "tigron/skeleton",
	"description": "Tigron Skeleton",
	"type": "project",
	"require": {
		"ext-soap": "*",

		"tigron/skeleton-database": "^0.3.10",
		"tigron/skeleton-object": "^1.0.1",
		"tigron/skeleton-file": "^2.0.4",
		"tigron/skeleton-file-picture": "0.2.12",
		"tigron/skeleton-file-pdf": "^0.2.8",
		"tigron/skeleton-i18n": "^3.0.3",
		"tigron/skeleton-pager": "^2.0.9",
		"tigron/skeleton-template-twig": "^2.0.12",
		"tigron/skeleton-core": "^2.0.10",
		"tigron/skeleton-error": "^0.2.5",
		"tigron/skeleton-template": "~0.1.5",
		"tigron/skeleton-email": "^0.3.7",
		"tigron/skeleton-console": "^2.0.1",
		"tigron/skeleton-migrate": "^0.1.16",
		"tigron/skeleton-package-api": "^1.0.1",
		"tigron/skeleton-package-crud": "^2.0.0",
		"tigron/skeleton-transaction": "1.1.2",
